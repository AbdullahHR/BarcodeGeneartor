<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/styels.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="barcodegenerator.UserHomePController">
   <children>
      <Label fx:id="titlelbl" layoutX="218.0" layoutY="49.0" prefHeight="32.0" prefWidth="240.0" text="Barcode Generator" />
      <ImageView fitHeight="89.0" fitWidth="91.0" layoutX="100.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/preview.png" />
         </image>
      </ImageView>
      <Button layoutX="90.0" layoutY="273.0" mnemonicParsing="false" onAction="#generate" text="Generate New Barcodes" />
      <ImageView fitHeight="100.0" fitWidth="89.0" layoutX="135.0" layoutY="163.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/refresh-barcode.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="89.0" fitWidth="102.0" layoutX="393.0" layoutY="170.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/view.png" />
         </image>
      </ImageView>
      <Button layoutX="343.0" layoutY="273.0" mnemonicParsing="false" onAction="#view" text="View Generated Barcodes" />
      <Button layoutX="550.0" layoutY="14.0" mnemonicParsing="false" onAction="#logout">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="57.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/on-off-button.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
